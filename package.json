{
  "name": "revocabulary",
  "version": "1.0.0",
  "main": "server.js",
  "author": "Mattstacey",
  "license": "MIT",
  "scripts": {
    "start": "node -r ts-node/register app/server.ts",
    "start:watch": "nodemon",
    "build": "tsc",
    "watch-server": "nodemon --watch 'app/**/*' -e ts,tsx --exec refresh"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "app"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "apollo-server-express": "^2.11.0",
    "class-validator": "^0.11.1",
    "cross-fetch": "^3.1.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^0.17.6",
    "typegoose-cursor-pagination": "^1.2.1"
  },
  "devDependencies": {
    "@typegoose/typegoose": "^6.4.0",
    "@types/express": "^4.14.0",
    "@types/graphql": "^14.5.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/mongoose": "^5.7.8",
    "@types/node": "^13.9.8",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": "13.2.0"
  }
}
